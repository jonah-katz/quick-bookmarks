{
  "name": "Quick Bookmarks",
  "description": "This extension brings up all your bookmarks in an easy to navigate fashion.",
  "version": "1.3",
 "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "webNavigation",
    "chrome://favicon/",
    "tabs"
   ],
    "manifest_version": 3,

  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"


}